<h1>Edit listings</h1>
<%= form_tag "/listings/#{@listing.id}", method: :patch do %>
  <div>
    <%= label_tag :name %>
    <%= text_field_tag :name, @listing.name %>
  </div>
    <div>
    <%= label_tag :address %>
    <%= text_field_tag :address, @listing.address %>
  </div>
    <%= submit_tag "Update listing" %>
  <%end%>
